{
  "fields": [
    {
      "name": "fiscal_yr",
      "title": "Année financière",
      "description": "Indique l'exercice financier (1 avril au 31 mars) durant lequel les activités du service ont eu lieu. Par exemple, les données pour l’exercice financier 2023-2024 devrait inclure les demandes de service qui ont été reçues entre le 1 avril 2023 et le 31 mars 2024.\n",
      "type": "string",
      "constraints": {
        "required": true,
        "enum": [
          "2005-2006",
          "2006-2007",
          "2007-2008",
          "2008-2009",
          "2009-2010",
          "2010-2011",
          "2011-2012",
          "2012-2013",
          "2013-2014",
          "2014-2015",
          "2015-2016",
          "2016-2017",
          "2017-2018",
          "2018-2019",
          "2019-2020",
          "2020-2021",
          "2021-2022",
          "2022-2023",
          "2023-2024",
          "2024-2025",
          "2025-2026",
          "2026-2027",
          "2027-2028",
          "2028-2029",
          "2029-2030"
        ]
      }
    },
    {
      "name": "service_id",
      "title": "Numéro d'identification du service",
      "description": "Le numéro unique attribué à un service dans le répertoire afin de faciliter le référencement à des services précis.",
      "type": "string",
      "constraints": {
        "required": true
      }
    },
    {
      "name": "service_name_fr",
      "title": "Nom du service (en français)",
      "description": "Indique le nom officiel du service.",
      "type": "string",
      "constraints": {
        "required": true
      }
    },
    {
      "name": "service_description_fr",
      "title": "Description du service (en français)",
      "description": "Offre une brève description du service, en langage simple.",
      "type": "string",
      "constraints": {
        "required": true
      }
    },
    {
      "name": "service_type",
      "title": "Type de service",
      "description": "Indique le type de service tel qu'indiqué dans la Ligne directrice sur les services et le numérique. Séparez les entrées par une virgule (,) s’il y en a plusieurs qui s’appliquent.",
      "type": "string",
      "constraints": {
        "required": true,
        "enum": [
          "APIR",
          "CER",
          "GNC",
          "INFO",
          "LRP",
          "PPI",
          "REG_VOL",
          "RES"
        ]
      }
    },
    {
      "name": "service_recipient_type",
      "title": "Type de bénéficiaire du service",
      "description": "Services ciblés axés sur les clients : Répondent aux besoins de clients ou de groupes particuliers, par exemple les personnes, les entreprises ou les employés du GC. Services non ciblés axés sur la société : Répondent aux besoins de la société en général et non aux besoins de personnes ou de groupes distincts, par exemple les forces armées ou la science pure.\n",
      "type": "string",
      "constraints": {
        "required": true,
        "enum": [
          "CLIENT",
          "SOCIETY"
        ]
      }
    },
    {
      "name": "service_scope",
      "title": "Étendue du service",
      "description": "Indique si le service est offert aux clients externes ou internes au gouvernement. Séparez les entrées par une virgule (,) s’il y en a plusieurs qui s’appliquent.",
      "type": "string",
      "constraints": {
        "required": true,
        "enum": [
          "CLUSTER",
          "ENTERPRISE",
          "EXTERN",
          "INTERN"
        ]
      }
    },
    {
      "name": "client_target_groups",
      "title": "Clients/groupes cibles",
      "description": "Identifie les clients ou les groupes de services cibles. Séparez les entrées par une virgule (,) s’il y en a plusieurs qui s’appliquent.",
      "type": "string",
      "constraints": {
        "required": true,
        "enum": [
          "ECONOM",
          "FOR",
          "INTERN_GOV",
          "NGO",
          "PERSON",
          "PTC"
        ]
      }
    },
